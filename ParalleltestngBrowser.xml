<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Parallel Browser Testing" parallel="tests">
  <test thread-count="5" name="Testing in Chrome">
	<parameter name="Browser" value="Chrome"></parameter>
	<classes>
		<class name="TestFile.AirLine_Paralleltesting"></class>
	</classes>
  </test> <!-- Test -->
  
  <test thread-count="5" name="Testing in Edge">
	<parameter name="Browser" value="edge"></parameter>
	<classes>
		<class name="TestFile.AirLine_Paralleltesting"></class>
	</classes>
  </test> <!-- Test -->
  
  <test thread-count="5" name="Testing in Firefox">
	<parameter name="Browser" value="firefox"></parameter>
	<classes>
		<class name="TestFile.AirLine_Paralleltesting"></class>
	</classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
